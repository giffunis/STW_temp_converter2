  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ethanmick/blanket-node.git"
  },
  "bugs": {
    "url": "https://github.com/ethanmick/blanket-node/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ethanmick/blanket-node/blob/develop/LICENSE"
    }
  ],
  "main": "bin/index.js",
  "engines": {
    "node": ">=0.10.x"
  },
  "dependencies": {
    "esprima": "1.2.2",
    "falafel": "0.3.1",
    "winston": "0.8.3",
    "xtend": "~3.0.0"
  },
  "devDependencies": {
    "async": "0.9.0",
    "chai": "1.10.0",
    "coffee-script": "1.9.0",
    "mocha": "2.1.0",
    "sinon": "1.12.2",
    "travis-cov": "*"
  },
  "scripts": {
    "prepublish": "./node_modules/coffee-script/bin/coffee --output bin --compile src/",
    "test": "make test; make travis-cov"
  },
  "config": {
    "blanket": {
      "pattern": "src",
      "data-cover-flags": {
        "debug": false
      },
      "loader": "./node-loaders/coffee-script",
      "data-cover-reporter-options": {
        "shortnames": true
      }
    },
    "travis-cov": {
      "threshold": 70,
      "removeKey": "branchFcn"
    }
  },
  "keywords": [
    "blanket",
    "blanket-node",
    "node-blanket",
    "code coverage",
    "code",
    "coverage"
  ],
