{
  "name": "blanket-node",
  "description": "Seamless JS and CS Code Coverage",
  "version": "2.0.0",
  "homepage": "https://github.com/ethanmick/blanket-node",
  "author": {
    "name": "Alex-Seville, Ethan Mick",
    "email": "ethan@ethanmick.com",
